{
	"name": "fuko-php/open",
	"license": "MIT",
	"description": "Generates links to open referenced files directly in your IDE or editor, or have it linked to an online repository",
	"keywords": ["editor", "link", "fuko", "fuko-php"],
	"authors": [
		{
		"name": "Kaloyan Tsvetkov (KT)",
		"email": "kaloyan@kaloyan.info",
		"homepage": "http://kaloyan.info",
		"role": "Developer"
		}
	],

	"require": {
		"php": ">=7.1"
	},

	"autoload": {
		"psr-4": {
			"Fuko\\Open\\": "src/"
		}
	},

	"require-dev": {
		"phpunit/phpunit": "^7.5 || ^8.5"
	},

        "autoload-dev": {
		"psr-4": {
			"Fuko\\Open\\Tests\\": "tests/"
		}
        }
}
